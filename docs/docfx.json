{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "src": "../src"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["*.md", "toc.yml"]
      }
    ],
    "dest": "dist",
    "globalMetadata": {
      "_appTitle": "Avolantis.Text.QueryString Documentation",
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "xrefService": ["https://xref.docs.microsoft.com/query?uid={uid}"]
  }
}
